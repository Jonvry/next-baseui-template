{
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "printWidth": 100,
    "trailingComma": "es5",
    "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
    "importOrder": [
        "server-only",
        "^(react)$",
        "^(next)",
        "<THIRD_PARTY_MODULES>",
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderTypeScriptVersion": "5.0",
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
